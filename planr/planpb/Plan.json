                "interfaces": {
                    "items": {
                        "$ref": "#/definitions/migration.Interface"
                    },
                    "type": "array"
                },
                "datastores": {
                    "items": {
                        "$ref": "#/definitions/migration.Datastore"
                    },
                    "type": "array"
