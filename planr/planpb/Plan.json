                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Plan",
            "description": "The top-level structure of the plan.yml file"
        },
        "migration.Application": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "modules": {
                    "items": {
                        "$ref": "#/definitions/migration.Module"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Application",
            "description": "ABB L2."
        },
        "migration.Change": {
            "properties": {
                "type": {
                    "type": "string"
                },
                "planned_release": {
                    "type": "string"
